<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://www.ifi.unizh.ch/ddis/evoont/2007/02/bom#"
    xmlns:vom="http://www.ifi.unizh.ch/ddis/evoont/2007/02/vom#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://www.ifi.unizh.ch/ddis/evoont/2007/02/bom">
   <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.ifi.unizh.ch/ddis/evoont/2007/02/vom"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Activity"/>
  <owl:Class rdf:ID="Person"/>
  <owl:Class rdf:ID="Attachment"/>
  <owl:Class rdf:ID="Issue"/>
  <owl:Class rdf:ID="Comment"/>
  <owl:Class rdf:ID="Component"/>
  <owl:Class rdf:ID="ComputerSystem"/>
  <owl:Class rdf:ID="Product"/>
  <owl:Class rdf:ID="Milestone"/>
  <owl:ObjectProperty rdf:ID="isCommentorOf">
    <rdfs:range rdf:resource="#Comment"/>
    <owl:inverseOf>
      <owl:FunctionalProperty rdf:ID="hasCommentor"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isCcPersonOf">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasCcPerson"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Issue"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="blocks">
    <rdfs:domain rdf:resource="#Issue"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="dependsOn"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Issue"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasCcPerson">
    <rdfs:domain rdf:resource="#Issue"/>
    <owl:inverseOf rdf:resource="#isCcPersonOf"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isResolutionOf">
    <rdfs:domain rdf:resource="http://www.ifi.unizh.ch/ddis/evoont/2007/02/vom#Revision"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasResolution"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Issue"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasComputerSystem">
    <rdfs:domain rdf:resource="#Issue"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isComputerSystemOf"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#ComputerSystem"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasResolution">
    <rdfs:range rdf:resource="http://www.ifi.unizh.ch/ddis/evoont/2007/02/vom#Revision"/>
    <rdfs:domain rdf:resource="#Issue"/>
    <owl:inverseOf rdf:resource="#isResolutionOf"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#dependsOn">
    <owl:inverseOf rdf:resource="#blocks"/>
    <rdfs:range rdf:resource="#Issue"/>
    <rdfs:domain rdf:resource="#Issue"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#isComputerSystemOf">
    <rdfs:range rdf:resource="#Issue"/>
    <owl:inverseOf rdf:resource="#hasComputerSystem"/>
    <rdfs:domain rdf:resource="#ComputerSystem"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="datatypeProperty_6"/>
  <owl:DatatypeProperty rdf:ID="keyword">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Issue"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="hasMilestone">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="isMilestoneOf"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#Issue"/>
    <rdfs:range rdf:resource="#Milestone"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isActivityOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Activity"/>
    <rdfs:range rdf:resource="#Issue"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasActivity"/>
    </owl:inverseOf>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="what">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Activity"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="removed">
    <rdfs:domain rdf:resource="#Activity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="added">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Activity"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="text">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Comment"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="name">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Product"/>
          <owl:Class rdf:about="#Component"/>
          <owl:Class rdf:about="#Milestone"/>
          <owl:Class rdf:about="#Person"/>
          <owl:Class rdf:about="#Attachment"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasReporter">
    <rdfs:domain rdf:resource="#Issue"/>
    <rdfs:range rdf:resource="#Person"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="isReporterOf"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="performed">
    <rdfs:domain rdf:resource="#Activity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="description">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Issue"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="type">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Attachment"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasCommentor">
    <owl:inverseOf rdf:resource="#isCommentorOf"/>
    <rdfs:range rdf:resource="#Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Comment"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="priority">
    <rdfs:domain rdf:resource="#Issue"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >P1</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >P2</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >P3</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >P4</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >P5</rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:rest>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isIssueOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Issue"/>
    <rdfs:range rdf:resource="#Component"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="platform">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#ComputerSystem"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasAssignee">
    <rdfs:range rdf:resource="#Person"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="isAssigneeOf"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#Issue"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="date">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Comment"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="severity">
    <rdfs:domain rdf:resource="#Issue"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >BLOCKER</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >CRITICAL</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >NORMAL</rdf:first>
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:rest>
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >ENHANCMENT</rdf:first>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >TRIVIAL</rdf:first>
                </rdf:rest>
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >MINOR</rdf:first>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >MAJOR</rdf:first>
            </rdf:rest>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isComponentOf">
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasComponent"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Component"/>
    <rdfs:range rdf:resource="#Product"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="number">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Comment"/>
          <owl:Class rdf:about="#Issue"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="email">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="status">
    <rdfs:domain rdf:resource="#Issue"/>
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >ASSIGNED</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:parseType="Resource">
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >REOPENED</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >UNCONFIRMED</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >VERIFIED</rdf:first>
                    </rdf:rest>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >RESOLVED</rdf:first>
                </rdf:rest>
              </rdf:rest>
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >NEW</rdf:first>
            </rdf:rest>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >CLOSED</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="fileName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Attachment"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isCommentOf">
    <rdfs:range rdf:resource="#Issue"/>
    <rdfs:domain rdf:resource="#Comment"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasComment"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="dateOpened">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:domain rdf:resource="#Issue"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="bugURL">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Issue"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="os">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#ComputerSystem"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="version">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Product"/>
          <owl:Class rdf:about="#Component"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasCreator">
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="isCreatorOf"/>
    </owl:inverseOf>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Comment"/>
          <owl:Class rdf:about="#Attachment"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasInvolvedPerson">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Person"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="isInvolvedPersonOf"/>
    </owl:inverseOf>
    <rdfs:domain rdf:resource="#Activity"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isAttachmentOf">
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:ID="hasAttachment"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="#Issue"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Attachment"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="target">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:domain rdf:resource="#Milestone"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="lastModified">
    <rdfs:domain rdf:resource="#Issue"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="resolution">
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:rest rdf:parseType="Resource">
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >FIXED</rdf:first>
            <rdf:rest rdf:parseType="Resource">
              <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
              >INVALID</rdf:first>
              <rdf:rest rdf:parseType="Resource">
                <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >MOVED</rdf:first>
                <rdf:rest rdf:parseType="Resource">
                  <rdf:rest rdf:parseType="Resource">
                    <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >WORKSFORME</rdf:first>
                    <rdf:rest rdf:parseType="Resource">
                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                      >LATER</rdf:first>
                      <rdf:rest rdf:parseType="Resource">
                        <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >REMIND</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      </rdf:rest>
                    </rdf:rest>
                  </rdf:rest>
                  <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >WONTFIX</rdf:first>
                </rdf:rest>
              </rdf:rest>
            </rdf:rest>
          </rdf:rest>
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >DUPLICATE</rdf:first>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Issue"/>
  </owl:FunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#hasComponent">
    <owl:inverseOf rdf:resource="#isComponentOf"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Component"/>
    <rdfs:domain rdf:resource="#Product"/>
  </owl:InverseFunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#hasComment">
    <rdfs:range rdf:resource="#Comment"/>
    <rdfs:domain rdf:resource="#Issue"/>
    <owl:inverseOf rdf:resource="#isCommentOf"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:InverseFunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#isInvolvedPersonOf">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="#Activity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="#hasInvolvedPerson"/>
  </owl:InverseFunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#isAssigneeOf">
    <owl:inverseOf rdf:resource="#hasAssignee"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Issue"/>
    <rdfs:domain rdf:resource="#Person"/>
  </owl:InverseFunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#isMilestoneOf">
    <owl:inverseOf rdf:resource="#hasMilestone"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="#Milestone"/>
    <rdfs:range rdf:resource="#Issue"/>
  </owl:InverseFunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#isCreatorOf">
    <rdfs:domain rdf:resource="#Person"/>
    <owl:inverseOf rdf:resource="#hasCreator"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Comment"/>
          <owl:Class rdf:about="#Attachment"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
  </owl:InverseFunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="#isReporterOf">
